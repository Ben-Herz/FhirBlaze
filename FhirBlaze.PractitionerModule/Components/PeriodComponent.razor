@namespace FhirBlaze.PractitionerModule.Components

<div class="row">
	<div class="form-group">
		<label class="control-label col-sm-3" for="PeriodStart">Period Start</label>
		<div class="col-sm-3">
			<input id="PeriodStart" type="date" @bind="PeriodStart" />
		</div>
	</div>
	
	<div class="form-group">
		<label class="control-label col-sm-3" for="PeriodEnd">Period End</label>
		<div class="col-sm-3">
			<input id="PeriodEnd" type="date" @bind="PeriodEnd" />
		</div>
	</div>
</div>